{
    "name": "rgustbardon/vectory",
    "description": "Lean typed vectors for PHP",
    "type": "library",
    "require": {
        "php": "^7.1",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "php": "^7.2",
        "friendsofphp/php-cs-fixer": "^2.14",
        "nikic/php-parser": "^4.2",
        "phpstan/phpstan": "^0.11.5",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpunit/phpunit": "^8",
        "psr/log": "^1.1",
        "symfony/process": "^4.2",
        "yay/yay": "dev-master"
    },
    "conflict": {
        "hhvm": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Gust-Bardon",
            "email": "robert@gust-bardon.org"
        }
    ],
    "autoload": {
        "Vectory\\": "dist/"
    },
    "autoload-dev": {
        "psr-4": {
            "Vectory\\Builder\\": "src/php/Builder/",
            "Vectory\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "build": ["php bin/build -v"]
    }
}
